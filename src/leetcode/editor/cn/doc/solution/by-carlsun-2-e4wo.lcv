class Solution {
public:
    ListNode* removeElements(ListNode* head, int val) {
        // åˆ é™¤å¤´ç»“ç‚¹
        while (head != NULL && head->val == val) { // æ³¨æ„è¿™é‡Œä¸æ˜¯if
            ListNode* tmp = head;
            head = head->next;
            delete tmp;
        }

        // åˆ é™¤éå¤´ç»“ç‚¹
        ListNode* cur = head;
        while (cur != NULL && cur->next!= NULL) {
            if (cur->next->val == val) {
                ListNode* tmp = cur->next;
                cur->next = cur->next->next;
                delete tmp;
            } else {
                cur = cur->next;
            }
        }
        return head;
    }
};
